{
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "jest": true
    },
    "globals": {
      "document": true
    },
    "rules": {
      "comma-dangle": 0,
      "class-methods-use-this" : 0,
      "import/no-extraneous-dependencies": 0
    }
  }
